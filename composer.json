{
    "name": "abc/wp-ecommerce",
    "description": "Composer settings for the project.",
    "type": "project",
    "require": {
        "php": "^8.0",
        "johnpbloch/wordpress": "^6.2",
        "wpackagist-plugin/akismet": "^5.0",
        "wpackagist-theme/twentytwentyone": "^2.0",
        "wpackagist-plugin/wp-redis": "^1.4",
        "wpackagist-plugin/wp-native-php-sessions": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "wp-ecommerce\\": "src/"
        }
    },
    "autoload-dev": {
    "psr-4": {
        "wp-ecommerce\\Tests\\": "tests/"
    }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "johnpbloch/wordpress-core-installer": true,
            "composer/installers": true
        }
    }
}
